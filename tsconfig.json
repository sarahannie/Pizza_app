{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "components/checkoutMedal.jsx", "pages/Checkout/CheckOut.jsx", "app/frontpage/checkout/page.jsx", "pages/admin/Home/home.jsx", "pages/login/Login.jsx", "app/login/page.jsx", "app/api/auth/[...nextauth]/route.js", "components/AppContext.jsx", "components/navbar.jsx", "pages/admin/getpizza/getpizza.jsx", "app/api/user/product/route.js", "app/(admin)/adminlanding/page.jsx", "app/(admin)/adminlanding/layout.tsx", "pages/admin/editpizza/editpizza.tsx", "pages/admin/Product/product.jsx", "pages/ProductDetail/productDetail.jsx", "pages/Cart/cart.jsx"],
  "exclude": ["node_modules"]
}
